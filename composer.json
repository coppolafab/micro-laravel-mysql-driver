{
    "name": "coppolafab/micro-laravel-mysql-driver",
    "description": "A minimal extension of Laravel default behaviour to minimize interactions after connection towards a MySql Server",
    "keywords": [
        "database",
        "mysql",
        "driver",
        "laravel"
    ],
    "homepage": "https://github.com/coppolafab/micro-laravel-mysql-driver",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Fabio Coppola",
            "homepage": "https://github.com/coppolafab"
        }
    ],
    "autoload": {
        "psr-4": {
            "coppolafab\\MicroMySqlDriver\\": "src"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "illuminate/support": "^5.7|^6.0|^7.0",
        "illuminate/database": "^5.7|^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "coppolafab\\MicroMySqlDriver\\MicroMySqlDriverServiceProvider"
            ]
        }
    }
}
